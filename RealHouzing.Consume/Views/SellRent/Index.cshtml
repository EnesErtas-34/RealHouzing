@using RealHouzing.Consume.Models.SellRentModels;
@model List<SellRentListViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<br />
<div class="container">
    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Başlık</th>
            <th>İkinci Başlık</th>
            <th>Resim</th>
            <th>Buton İsim</th>
            <th>Sil</th>
            <th>Güncelle</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.SellRentID</td>
                <td>@item.Title</td>
                <td>@item.Title2</td>
                <td><img src="@item.ImageURL" style="height:75px;width:75px" /></td>
                <td>@item.Button</td>
                <td><a href="/SellRent/DeleteSellRent/@item.SellRentID" class="btn btn-danger">Sil</a></td>
                <td><a href="/SellRent/UpdateSellRent/@item.SellRentID" class="btn btn-success">Güncelle</a></td>

            </tr>
        }
    </table>
    <br />
    <a href="/SellRent/AddSellRent/" class="btn btn-primary">Yeni Sat&Kirala Girişi</a>
</div>

